block variables

  doctype html
  html(lang= 'ru')
    head
      meta(charset= 'utf-8')
      meta(name= 'viewport' content= 'width=device-width, initial-scale=1')
      meta(name= 'description' content= description)
      link(rel='stylesheet' href=urlCss)
      script(defer src=urlJs)
      title= title

    body
      block main
        .content
          .article
            .creationDate= `Created: ${create || date}`
            != contentHtml